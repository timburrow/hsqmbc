<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Proc2Dbasic" element="pages" type="processing" >
  <group size="750 295"
    style="PlainText"
    label="proc/plot"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Proc2Dbasic"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 35" size="215 205"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="25 50" size="160 20"
        style="Label1"
        label="Default Linear Prediction"
        vc="setLP1 sqsinebell('f2') gaussian('f1')"
        decor1="No"
        />
      <button loc="25 30" size="160 20"
        style="Label1"
        label="Default Weighting"
        vc="sqsinebell('f2') gaussian('f1')"
        decor1="No"
        />
      <label loc="40 5" size="130 20"
        style="Label4"
        label="Process Options"
        justify="Center"
        />
    </group>
    <group loc="225 35" size="270 205"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="dispbasic"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <radio loc="200 130" size="50 20"
        style="PlainText"
        label="F2"
        vc="trace='f2' dconi"
        vc2="trace='f1' dconi"
        />
      <radio loc="150 130" size="50 20"
        style="PlainText"
        label="F1"
        vc="trace='f1' dconi"
        vc2="trace='f2' dconi"
        />
      <label loc="145 105" size="105 25"
        style="Label1"
        label="Horizontal Axis"
        />
      <button loc="145 70" size="110 20"
        style="Label3"
        label="Full Spectrum"
        vc="f dconi"
        decor1="No"
        />
      <button loc="145 50" size="110 20"
        style="Label3"
        label="Screen w/ proj"
        vc="fullt dconi"
        decor1="No"
        />
      <button loc="145 30" size="110 20"
        style="Label3"
        label="Full Screen"
        vc="full dconi"
        decor1="No"
        />
      <group loc="10 30" size="120 135"
        style="PlainText"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <button loc="10 110" size="100 20"
          style="Label1"
          label="Reference (F1)"
          vc="reff1"
          decor1="No"
          />
        <label loc="0 60" size="100 20"
          style="Label1"
          label="set F2 cursor to:"
          />
        <button loc="10 40" size="100 20"
          style="Label3"
          label="By TMS"
          vc="setref tmsref sp=sp-rfl dconi"
          decor1="No"
          />
        <button loc="10 20" size="100 20"
          style="Label3"
          label="By Solvent"
          vc="setref sp=sp-rfl dconi"
          decor1="No"
          />
        <label loc="10 0" size="100 20"
          style="Label1"
          label="Reference (F2)"
          justify="Center"
          />
        <menu loc="50 80" size="60 20"
          style="PlainText"
          label="menu"
          vq="ref"
          vc="parunits('set','ref','$VALUE')"
          set="parunits('get','ref'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="ppm"
            chval="ppm1"
            />
          <mlabel 
            label="Hz"
            chval="Hz"
            />
        </menu>
        <entry loc="0 80" size="50 20"
          style="PlainText"
          vq="ref"
          vc="vnmrunits('set','ref',$VALUE) rl(ref) dconi"
          set="vnmrunits('get','ref'):$VALUE"
          show="on('ref'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <label loc="70 5" size="130 20"
        style="Label4"
        label="Display Options"
        justify="Center"
        />
    </group>
    <group loc="500 35" size="190 205"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="plottemp"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="15 150" size="160 20"
        style="Heading3"
        label="Preview plot"
        vq="PDFpreview"
        vc="exec(parstyle) exec(pkpick)  pageview"
        show="$ENABLE=(PDFpreview='y')*2-1"
        bg="tan"
        decor1="Yes"
        />
      <button loc="15 130" size="160 20"
        style="Heading3"
        label="Clear "
        vc="page('clear')"
        bg="tan"
        decor1="Yes"
        />
      <button loc="15 105" size="160 25"
        style="Heading3"
        label="Send2plotter"
        vc="exec(parstyle) exec(pkpick)  page "
        bg="tan"
        decor1="Yes"
        />
      <menu loc="95 70" size="90 20"
        style="Label3"
        label="menu"
        vq="parstyle"
        vc="parstyle='$VALUE'"
        set="$VALUE=parstyle"
        editable="No"
        >
        <mlabel 
          label="Basic (left)"
          chval="ppa"
          />
        <mlabel 
          label="Basic (right)"
          chval="ppa(wc-50)"
          />
        <mlabel 
          label="Full (left)"
          chval="pap"
          />
        <mlabel 
          label="Full (right)"
          chval="pap(wc-50)"
          />
        <mlabel 
          label="None"
          />
      </menu>
      <label loc="5 70" size="85 20"
        style="Label3"
        label="parameters"
        justify="Right"
        />
      <button loc="95 45" size="90 20"
        style="Label3"
        label="Logo"
        vc="pllogo"
        show="is_logo:$ret $ENABLE=($ret&gt;0.5)*2-1"
        decor1="No"
        />
      <button loc="5 45" size="90 20"
        style="Label3"
        label="Molecule"
        vc="imagefile('plotall')"
        show="exists('/vnmr/java/jchempaint.jar','file'):$jchem $ENABLE=($jchem&gt;0.5)*2-1"
        decor1="No"
        />
      <button loc="5 25" size="90 20"
        style="Label3"
        label="Spectrum"
        vc="plot2D"
        decor1="No"
        />
      <button loc="95 25" size="90 20"
        style="Label3"
        label="Text"
        vc="pltext"
        decor1="No"
        />
      <label loc="25 6" size="130 20"
        style="Label4"
        label="Manual plot"
        justify="Center"
        />
    </group>
    <group loc="165 245" size="385 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <button loc="7 0" size="370 20"
        style="Label3"
        label="Save Current Process/Display parameters"
        vq="procdim"
        vc="svpdp"
        show="isnewexp:$isnew if (($isnew=0) and (procdim&gt;0)) then $ENABLE=1 else $ENABLE=-1 endif"
        decor1="Yes"
        />
    </group>
    <button loc="525 5" size="160 25"
      style="Heading3"
      label="Auto Plot preview"
      vq="PDFpreview"
      vc="pageview('auto')"
      show="$ENABLE=(PDFpreview='y')*2-1"
      bg="tan"
      decor1="Yes"
      />
    <button loc="355 5" size="160 25"
      style="Heading3"
      label="Automatic Plot"
      vc="plot"
      bg="tan"
      decor1="Yes"
      />
    <button loc="185 5" size="160 25"
      style="Heading3"
      label="Display"
      vc="dconi"
      bg="tan"
      decor1="Yes"
      />
    <button loc="15 5" size="160 25"
      style="Heading3"
      label="Process"
      vc="CPdndfid_process"
      bg="tan"
      decor1="Yes"
      />
  </group>
</template>
